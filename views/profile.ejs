<h2>Profile Page</h2>

<a href="/">Home</a>
<a href="/recipes">MyRecipes</a>


<% bubble.forEach(function(recipe) { %>
    <h2><%= recipe.title %></h2>
    <a href="<%= recipe.href %>">recipe</a>
    <p><%= recipe.ingredients %></p>
    <img src=<%= recipe.thumbnail %> alt="">

<form action="/recipes" method="post">
    <button type="submit">add to MyRecipes</button>
</form>

<% }); %>
